<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Dialog title="Details" titleAlignment="Center" contentWidth="75%" contentHeight="auto" stretch="{device>/system/phone}">
        <content>
            <Table id="idTestTable" sticky="ColumnHeaders" visible="{= (${device>/system/desktop} || ${device>/system/tablet}) ? true : false}">
                
            </Table>
            <List id="idTestList" showSeparators="Inner" visible="{device>/system/phone}">
                
            </List>
            <!-- <VBox id="idTestVbox">
                
            </VBox> -->
        </content>
        <endButton>
            <Button text="Close" press="onCloseTestDialog"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>