<core:FragmentDefinition xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
    <Dialog title="Apply filter" titleAlignment="Center" contentWidth="30%" stretch="{device>/system/phone}">
        <content>
            <f:SimpleForm id="idRegionFilterForm" editable="true" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="3" emptySpanL="3" emptySpanM="0" emptySpanS="0" columnsXL="6" columnsL="6" columnsM="12" singleContainerFullSize="false">
                <f:content>
                    <Label text="Region" />
                    <MultiComboBox id="idSelectRegions" showSelectAll="true">
                        <items>
                            <!-- <core:Item text="{DemoData>region}" key="{DemoData>region}"/> -->
                        </items>
                    </MultiComboBox>
                    <Label text="Company Code" />
                    <MultiComboBox id="idSelectCompCode" showSelectAll="true">
                        <items>
                            <!-- <core:Item text="{DemoData>name} - {DemoData>description}" key="{DemoData>name}"/> -->
                        </items>
                    </MultiComboBox>
                    <Label text="Distribution Center" />
                    <MultiComboBox id="idSelectDistCenter" showSelectAll="true">
                        <items>
                            <!-- <core:Item text="{DemoData>name} - {DemoData>description}" key="{DemoData>name}"/> -->
                        </items>
                    </MultiComboBox>
                    <Label text="Vendors" />
                    <MultiComboBox id="idSelectVendor" showSelectAll="true">
                        <items>
                            <!-- <core:Item text="{DemoData>name} - {DemoData>description} " key="{DemoData>name}"></core:Item> -->
                        </items>
                    </MultiComboBox>
                    <Label text="Season" />
                    <MultiComboBox id="idSelectSeason" showSelectAll="true">
                        <items>
                            <!-- <core:Item text="{DemoData>name}" key="{DemoData>name}"></core:Item> -->
                        </items>
                    </MultiComboBox>
                </f:content>
            </f:SimpleForm>
        </content>
        <beginButton>
            <Button text="Apply" type="Emphasized" press="onApplyMobileFilter"/>
        </beginButton>
        <endButton>
            <Button text="Cancel" press="onCancelMobileFilter"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>